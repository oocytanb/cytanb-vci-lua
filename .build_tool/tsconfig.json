{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "rootDir": ".",
    "paths": {
      "luamin": ["./luamin"]
    },
    "outDir": "./dist/",
    "strict": true,
    "moduleResolution": "node",
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf"
  },
  "include": ["*"]
}
